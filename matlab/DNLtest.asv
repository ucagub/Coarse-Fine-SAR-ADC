samples = 100;
mismatch = 0.05;

dacs = [];
for k = 1:samples
    dacs = [dacs multistep_CS('test', mismatch)];
    k
end

dnls = [];
inls = zeros(samples,255);
for k = 1:samples
    dnls = [dnls; dacs(k).DNL];
    for u = 1:255
        for v = 1:u
            inls(k,u) = inls(k,u) + dnls(k,v);
        end
    end
end


DNL_var = [];
INL_var = []
for i = 1:255
    DNL_var = [DNL_var var(dnls(:,i))];
    DNL_var = [DNL_var var(dnls(:,i))];
end

plot(DNL_var);

